<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">
    <!--
        Added the constraints for entity UserAnswers.
    -->
    <changeSet id="20220630094444-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="question_id"
                                 baseTableName="user_answers"
                                 constraintName="fk_user_answers__question_id"
                                 referencedColumnNames="id"
                                 referencedTableName="question"/>

        <addForeignKeyConstraint baseColumnNames="user_campaign_id"
                                 baseTableName="user_answers"
                                 constraintName="fk_user_answers__user_campaign_id"
                                 referencedColumnNames="id"
                                 referencedTableName="user_campaign"/>
    </changeSet>
</databaseChangeLog>
